{
  "name": "nooblyjs-core",
  "version": "1.0.8",
  "description": "A powerful set of modular Node.js backend services with singleton pattern - caching, logging, data serving, filing, measuring, notifying, queueing, scheduling, searching, workflow, and working services.",
  "keywords": [
    "nodejs",
    "backend",
    "services",
    "microservices",
    "cache",
    "logging",
    "queue",
    "workflow",
    "express"
  ],
  "files": [
    "src",
    "README.md",
    "package.json",
    "index.js"
  ],
  "license": "ISC",
  "author": "NooblyJS",
  "type": "commonjs",
  "main": "index.js",
  "engines": {
    "node": ">=12.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nooblyjs/nooblyjs-core.git"
  },
  "scripts": {
    "start": "node ./app-auth.js",
    "dev:web": "nodemon --watch ./src ./app-auth.js",
    "dev:web:noauth": "nodemon --watch ./src ./app-noauth.js",
    "test": "jest",
    "test-load": "node ./tests-load/",
    "kill": "kill -9 $(lsof -t -i:3001)",
    "analyze-tokens": "node ./scripts/count-tokens.js"
  },
  "jest": {
    "modulePaths": [
      "./"
    ],
    "transform": {},
    "forceExit": true,
    "detectOpenHandles": true
  },
  "devDependencies": {
    "jest": "^30.0.3",
    "nock": "^13.5.6"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.81",
    "@anthropic-ai/sdk": "^0.60.0",
    "@aws-sdk/client-s3": "^3.614.0",
    "@google-cloud/storage": "^7.17.0",
    "@google/gemini-cli": "^0.1.22",
    "aws-sdk": "^2.1620.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "ftp": "^0.3.10",
    "ioredis": "^5.6.1",
    "ioredis-mock": "^8.9.0",
    "memjs": "^1.3.0",
    "mongodb": "^6.19.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "openai": "^5.16.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^5.5.6",
    "rimraf": "^6.0.1",
    "simple-git": "^3.28.0",
    "supertest": "^7.1.4",
    "tslib": "^2.6.2",
    "uuid": "^11.1.0"
  }
}
